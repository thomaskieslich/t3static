{
	"name": "thomask/t3static",
	"description": "Collection of Tools to Test, Lint and Upgrade multiple TYPO3 Extensions.",
	"license": "GNU GENERAL PUBLIC LICENSE",
	"type": "library",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true,
		"allow-plugins": {
			"ergebnis/composer-normalize": true,
			"a9f/fractor-extension-installer": true
		}
	},
	"require-dev": {
		"a9f/typo3-fractor": "^0.5.0",
		"friendsofphp/php-cs-fixer": "^3.88.0",
		"helmich/typo3-typoscript-lint": "3.3.0",
		"michielroos/typo3scan": "*",
		"phpstan/phpstan": "^2.1.0",
		"seld/jsonlint": "^1.11.0",
		"ssch/typo3-rector": "^3.7.0",
		"symfony/yaml": "^7.3.0",
		"typo3/coding-standards": "0.8.0"
	}
}
