{
	"name": "thomask/t3static",
	"description": "Collection of Tools to Test, Lint and Upgrade multiple TYPO3 Extensions.",
	"license": "GPL-3.0-or-later",
	"type": "library",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"allow-plugins": {
			"ergebnis/composer-normalize": true,
			"a9f/fractor-extension-installer": true
		},
		"sort-packages": true
	},
	"require-dev": {
		"a9f/typo3-fractor": "v0.5.8",
		"ergebnis/composer-normalize":"2.48.2",
		"friendsofphp/php-cs-fixer": "3.91.3",
		"helmich/typo3-typoscript-lint": "v3.3.0",
		"michielroos/typo3scan": "dev-master",
		"phpstan/phpstan": "2.1.33",
		"seld/jsonlint": "1.11.0",
		"ssch/typo3-rector": "3.9.0",
		"symfony/yaml": "7.4.0",
		"typo3/coding-standards": "v0.8.0",
	}
}
