{
	"name": "thomask/t3static",
	"description": "Collection of Tools to Test, Lint and Upgrade multiple TYPO3 Extensions.",
	"license": "GNU GENERAL PUBLIC LICENSE",
	"type": "library",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true,
		"allow-plugins": {
			"ergebnis/composer-normalize": true,
			"a9f/fractor-extension-installer": true
		}
	},
	"require-dev": {
		"a9f/typo3-fractor": "v0.5.6",
		"friendsofphp/php-cs-fixer": "3.88.2",
		"helmich/typo3-typoscript-lint": "v3.3.0",
		"michielroos/typo3scan": "dev-master",
		"phpstan/phpstan": "2.1.31",
		"seld/jsonlint": "1.11.0",
		"ssch/typo3-rector": "v3.6.2",
		"symfony/yaml": "v7.3.3",
		"typo3/coding-standards": "v0.8.0"
	}
}
